{
    "files.autoSave": "afterDelay",
    "prettier.eslintIntegration": true,
    "editor.formatOnSave": true,
    "eslint.autoFixOnSave": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        {
            "language": "vue",
            "autoFix": true
        }
    ],
    "files.associations": {
        "*.vue": "vue"
    },
}